{
  "description": "Duplicates the current tab",
  "homepage_url": "https://github.com/Skeletonxf/duplicate-tab/",
  "manifest_version": 2,
  "name": "Duplicate tab",
  "version": "1.7",

  "permissions": ["contextMenus", "storage"],

  "background": {
    "scripts": ["/core/util.js", "/settings/defaults.js", "background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": {
       "16": "icons/fork-16.png",
       "32": "icons/fork-32.png",
       "64": "icons/fork-64.png"
    },
    "default_title": "Duplicate tab"
  },

  "options_ui": {
    "page": "/settings/settings.html"
  },

  "commands": {
    "duplicate-shortcut-1": {
      "suggested_key": {
        "default": "Ctrl+Shift+D"
       },
      "description": "Duplicate current tab 1"
    },
    "duplicate-shortcut-2": {
      "suggested_key": {
        "default": "Alt+Shift+D"
       },
      "description": "Duplicate current tab 2"
    },
    "duplicate-shortcut-3": {
      "description": "Duplicate current tab 3"
    }
  }
}
